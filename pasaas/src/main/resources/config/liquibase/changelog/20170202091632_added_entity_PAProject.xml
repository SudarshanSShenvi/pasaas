<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>
    <property name="now" value="GETDATE()" dbms="mssql"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle,mssql"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity PAProject.
    -->
    <changeSet id="20170202091632-1" author="jhipster">
        <createTable tableName="pa_project">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="projectname" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            
            <column name="description" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="paorgpro_id" type="bigint">
                <constraints nullable="false" />
            </column>
            
             <column name="pastatus" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="objectentity" type="varchar(255)">
                <constraints nullable="true" />
            </column>
			 <column name="timeseriesentity" type="varchar(255)">
                <constraints nullable="true" />
            </column>
 			<column name="feedindateformat" type="varchar(255)">
                <constraints nullable="true" />
            </column>
 			<column name="feedoutdateformat" type="varchar(255)">
                <constraints nullable="true" />
            </column>
			<column name="feedfirstlineheader" type="varchar(10)">
                <constraints nullable="true" />
            </column>
 			<column name="feedskipindexes" type="varchar(255)">
                <constraints nullable="true" />
            </column>
 			<column name="rollindateformat" type="varchar(255)">
                <constraints nullable="true" />
            </column>
 			<column name="rolloutdateformat" type="varchar(255)">
                <constraints nullable="true" />
            </column>
 			<column name="rollseriesgroupindex" type="varchar(255)">
                <constraints nullable="true" />
            </column>
 			<column name="rollseriesstart" type="timestamp">
                <constraints nullable="true" />
            </column>
 			<column name="rollseriesend" type="timestamp">
                <constraints nullable="true" />
            </column>
 			<column name="rollseriesnxt" type="timestamp">
                <constraints nullable="true" />
            </column>

 			<column name="patternfldindex" type="integer">
                <constraints nullable="true" />
            </column>
 			<column name="patterntraininterval" type="integer">
                <constraints nullable="true" />
            </column>
 			<column name="patternpredictinterval" type="integer">
                <constraints nullable="true" />
            </column>
 			<column name="patternintervalthreshhold" type="integer">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
</databaseChangeLog>
